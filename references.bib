	doi = {10.1146/annurev-control-060117-105226},
	abstract = {Robots with many degrees of freedom (e.g., humanoid robots and mobile manipulators) have increasingly been employed to accomplish realistic tasks in domains such as disaster relief, spacecraft logistics, and home caretaking. Finding feasible motions for these robots autonomously is essential for their operation. Sampling-based motion planning algorithms have been shown to be eﬀective for these high-dimensional systems. However, incorporating task constraints (e.g., keeping a cup level, writing on a board) into the planning process introduces signiﬁcant challenges. This survey describes the families of methods for sampling-based planning with constraints and places them on a spectrum delineated by their complexity. Constrained sampling-based methods are based upon two core primitive operations: () sampling constraint-satisfying conﬁgurations and () generating constraintsatisfying continuous motion. Although the basics of samplingbased planning are presented for contextual background, the survey focuses on the representation of constraints and samplingbased planners that incorporate constraints.},
	language = {en},
	number = {1},
	urldate = {2020-01-26},
	journal = {Annual Review of Control, Robotics, and Autonomous Systems},
	author = {Kingston, Zachary and Moll, Mark and Kavraki, Lydia E.},
	month = may,
	year = {2018},
	pages = {159--185},
	annote = {really useful for understanding how ompl works for sampling and state validation},
	file = {Kingston et al. - 2018 - Sampling-Based Methods for Motion Planning with Co.pdf:/Users/robertshi/Zotero/storage/JN822CHD/Kingston et al. - 2018 - Sampling-Based Methods for Motion Planning with Co.pdf:application/pdf}
}

